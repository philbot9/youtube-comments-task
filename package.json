{
  "name": "youtube-comments-task",
  "version": "1.3.5",
  "description": "Scrape comments from YouTube Videos.",
  "main": "./dist/index.js",
  "scripts": {
    "build": "babel src -d dist",
    "coverage": "nyc --reporter=html mocha src/test/*.js src/test/lib/*.js src/test/lib/**/*.js && xdg-open ./coverage/index.html",
    "format": "prettier-standard './src/**/*.js'",
    "integration-test": "mocha dist/test/integration-tests/*.js",
    "prepublish": "npm run build",
    "test": "npm run build && npm run unit-test && npm run integration-test",
    "unit-test": "mocha dist/test/*.js dist/test/lib/*.js dist/test/lib/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/philbot9/youtube-comments-task.git"
  },
  "keywords": [
    "youtube",
    "comment",
    "scraper",
    "task",
    "functional"
  ],
  "author": "Philip Klostermann <philip.klostermann@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/philbot9/youtube-comments-task/issues"
  },
  "homepage": "https://github.com/philbot9/youtube-comments-task#readme",
  "dependencies": {
    "cheerio": "^0.22.0",
    "control.monads": "^0.6.0",
    "core.lambda": "^1.0.0",
    "data.either": "^1.4.0",
    "data.task": "^3.1.1",
    "debug": "^2.2.0",
    "moment": "^2.17.1",
    "propper": "^1.0.2",
    "ramda": "^0.23.0",
    "request": "^2.74.0",
    "retry-task": "^1.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-env": "^1.2.1",
    "chai": "^3.5.0",
    "mocha": "^3.2.0",
    "nyc": "^10.1.2",
    "prettier-standard": "^5.0.0",
    "standard": "^8.6.0",
    "standard-format": "^2.2.4",
    "testdouble": "^1.10.1"
  }
}